{
  "name": "Wikipedia Age Obfuscator",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Obfuscates age on Wikipedia",
  "author": "Jacob Meacham",
  "homepage_url": "jemonjam.com",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["src/reload.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*wikipedia.org/*",
        "https://*wikipedia.org/*"
      ],
      "js": [
        "dist/extension.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
